<Project Sdk="Dalamud.NET.Sdk/11.0.0">
	<PropertyGroup>
        <Authors>HappySprite</Authors>
        <Version>0.0.0</Version>
        <Description>FFXIV Gathering Optimizer.</Description>
        <Copyright>Copyright Â© HappySprite 2024</Copyright>
        <PackageProjectUrl>https://github.com/belzaru17/GatheringOptimizer</PackageProjectUrl>
        <RepositoryUrl>https://github.com/belzaru17/GatheringOptimizer</RepositoryUrl>
        <OutputPath>bin\$(Configuration)\</OutputPath>
